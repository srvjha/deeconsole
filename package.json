{
  "name": "deeconsole",
  "version": "1.0.1",
  "description": "CLI to strip or comment console statements from project files",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "deeconsole": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc --pretty",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "prepare": "pnpm build"
  },
  "keywords": [
    "cli",
    "console",
    "cleanup"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.12.1",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/srvjha/deconsole.git"
  },
   "bugs": {
    "url": "https://github.com/srvjha/deconsole/issues"
  },
  "dependencies": {
    "@babel/parser": "^7.26.3",
    "@babel/traverse": "^7.26.3",
    "@babel/types": "^7.26.3",
    "commander": "^12.1.0",
    "fast-glob": "^3.3.2",
    "magic-string": "^0.30.10"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.18.4",
    "@types/node": "^22.7.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
